  <data name="ManifestText.Text" xml:space="preserve">
    <value>&lt;dotnetnuke type="Package" version="5.0"&gt;
  &lt;packages&gt;
    &lt;package name="{0}" type="Module" version="01.00.00"&gt;
      &lt;friendlyName&gt;{0}&lt;/friendlyName&gt;
      &lt;description&gt;{1}&lt;/description&gt;
      &lt;owner&gt;
        &lt;name/&gt;
        &lt;organization/&gt;
        &lt;url/&gt;
        &lt;email/&gt;
      &lt;/owner&gt;
      &lt;license src="license.txt"/&gt;
      &lt;releaseNotes src="releaseNotes.txt" /&gt;
      &lt;dependencies&gt;
        &lt;dependency type="coreversion"&gt;07.00.00&lt;/dependency&gt;
        &lt;dependency type="type"&gt;System.Tuple&lt;/dependency&gt;
      &lt;/dependencies&gt;
      &lt;components&gt;
        &lt;component type="Module"&gt;
          &lt;desktopModule&gt;
            &lt;moduleName&gt;{0}&lt;/moduleName&gt;
            &lt;foldername&gt;RazorModules/{2}&lt;/foldername&gt;
            &lt;businessControllerClass /&gt;
            &lt;supportedFeatures /&gt;
            &lt;moduleDefinitions&gt;
              &lt;moduleDefinition&gt;
                &lt;friendlyName&gt;{0}&lt;/friendlyName&gt;
                &lt;defaultCacheTime&gt;0&lt;/defaultCacheTime&gt;
                &lt;moduleControls&gt;
                  &lt;moduleControl&gt;
                    &lt;controlKey /&gt;
                    &lt;controlSrc&gt;DesktopModules/RazorModules/{2}/{3}&lt;/controlSrc&gt;
                    &lt;supportsPartialRendering&gt;False&lt;/supportsPartialRendering&gt;
                    &lt;controlTitle /&gt;
                    &lt;controlType&gt;View&lt;/controlType&gt;
                    &lt;iconFile /&gt;
                    &lt;helpUrl /&gt;
                    &lt;viewOrder&gt;0&lt;/viewOrder&gt;
                  &lt;/moduleControl&gt;
                &lt;/moduleControls&gt;
              &lt;/moduleDefinition&gt;
            &lt;/moduleDefinitions&gt;
          &lt;/desktopModule&gt;
        &lt;/component&gt;
        &lt;component type="File"&gt;
          &lt;files&gt;
            &lt;basePath&gt;DesktopModules\RazorModules\{2}&lt;/basePath&gt;
            &lt;file&gt;
              &lt;name&gt;{3}&lt;/name&gt;
            &lt;/file&gt;
            &lt;file&gt;
              &lt;name&gt;{4}&lt;/name&gt;
            &lt;/file&gt;
          &lt;/files&gt;
        &lt;/component&gt;
      &lt;/components&gt;
    &lt;/package&gt;
  &lt;/packages&gt;
&lt;/dotnetnuke&gt;</value>
